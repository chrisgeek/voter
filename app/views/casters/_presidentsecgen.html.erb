<div class="row">
  <div class="col-sm-6">

<table class='table table-striped' id="account_table_admin">
<thead>
  <tr><th class='theading' colspan=2><span class="glyphicon glyphicon-flag"></span> Director of Welfare's Ballot</th></tr>
</thead>
<tbody>
<% @aspirants.each do |aspirant| %>

<%if aspirant.position=="Director of Welfare"%>
<tr>
<td>
<%= image_tag aspirant.image_url(),class:"img-thumbnail",width:"100", height:"80"%>
 <%= aspirant.first_name%> <%=aspirant.last_name%>
 <span class="glyphicon glyphicon-tag" style='color:#74b6be'> </span> <%=aspirant.level%>
 </td>
 <td><div class="radio"> <%= f.radio_button :president, aspirant.id,class:'bigradio' %> </div> </td>
 </tr>
</tbody>
<%end%>
<%end%>

</table>
</div>

  <div class="col-sm-6">

