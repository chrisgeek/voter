$("ul.errors").html("")
<% if @user.errors.any? %>

   <% @user.errors.full_messages.each do |msg| %>

      $("ul.errors").append($("<li />").html("<%= msg.html_safe %>"))

    <% end %>
    $(".loading").hide()
<% else %>

 $(".render-ajax").html("<%= escape_javascript(render partial:"paginationusers",object:@users) %>")

 $("#subscription-modal").modal("hide")

 $(".loading").hide()

<% end %>