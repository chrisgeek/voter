$("ul.errors").html("")
<% if @voter.errors.any? %>

   <% @voter.errors.full_messages.each do |msg| %>

      $("ul.errors").append($("<li />").html("<%= msg.html_safe %>"))

    <% end %>
<% else %>

 $(".render-ajax").html("<%= escape_javascript(render partial:"paginationvoters",object:@voters) %>")

 $("#subscription-modal").modal("hide")

<% end %>