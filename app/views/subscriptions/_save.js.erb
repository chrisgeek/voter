$("ul.errors").html("")
<% if @subscription.errors.any? %>

   <% @subscription.errors.full_messages.each do |msg| %>

      $("ul.errors").append($("<li />").html("<%= msg.html_safe %>"))

    <% end %>
<% else %>

 $(".render-ajax").html("<%= escape_javascript(render partial:"paginationsubscribers",object:@subscriptions) %>")
 $("#subscription-modal").modal("hide")

<% end %>