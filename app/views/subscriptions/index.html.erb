<div class='wrapper'>

<%= render "shared/nav_bar" %>





<div class="row">
  <div class="col-sm-12">
<h3 class='voting_title'><span class="glyphicon glyphicon-envelope"></span> Voting Subscriptions</h3>
  <div class="form-group">
  <%= form_tag subscriptions_path,method: :get,remote:true do  %>
     <div class="input-group input-group-lg">
     <span class="input-group-addon">
        <span class="glyphicon glyphicon-search"></span>
      </span>
  <%= text_field_tag :search, "", class: 'form-control' %>
  </div>

  <% end %> 
</div>

<div class='render-ajax'>

<%= render 'paginationsubscribers' %>

</div>

<div class='new'>
<%= link_to "New Subscription", new_subscription_path, remote: true, class: "btn btn-primary" %>
</div>

<br>

  </div>

<%= render "shared/loading" %>
</div>
<div id="subscription-modal" class="modal fade"></div>
</div>



